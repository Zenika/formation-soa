<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:asynctimer="http://bpel.resanet.com/asynctimer">
  <process name="asynctimer:AsynchroneTimer">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="asynctimer:AsynchroneTimerService" port="AsynchroneTimerPort"/>
    </provide>
    <invoke partnerLink="client">
      <service name="asynctimer:AsynchroneTimerCallbackService" port="AsynchroneTimerCallbackPort"/>
    </invoke>
  </process>
</deploy>