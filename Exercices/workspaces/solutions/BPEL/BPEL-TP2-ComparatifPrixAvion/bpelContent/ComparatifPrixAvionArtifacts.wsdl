			<input message="tns:ComparatifPrixAvionRequestMessage" />
			<output message="tns:ComparatifPrixAvionResponseMessage" />
		</operation>
	</portType>






	<!--
		~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
		PARTNER LINK TYPE DEFINITION
		~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	-->
	<plnk:partnerLinkType name="ComparatifPrixAvion">
