	</plnk:partnerLinkType>
	<plnk:partnerLinkType name="DiscountParnerLinkType">
		<plnk:role name="Provider" portType="wsdl:CieDiscountPortType" />
	</plnk:partnerLinkType>
	<plnk:partnerLinkType name="CherePartnerLinkType">
		<plnk:role name="Provider" portType="wsdl1:CieCherePortType" />
	</plnk:partnerLinkType>


	<binding name="ComparatifPrixAvionBinding" type="tns:ComparatifPrixAvion">
		<soap:binding style="document"
